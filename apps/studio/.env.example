# Gemini AI API Key
# Get your API key from https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration
# Create a new project at https://supabase.com/dashboard
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Database URL (from Supabase project settings > Database > Connection string)
DATABASE_URL=postgresql://postgres:password@db.your-project.supabase.co:5432/postgres

# Site URL (for auth redirects)
NEXT_PUBLIC_SITE_URL=http://localhost:3000

# Docker Socket Path (optional, for Docker-based sandboxes)
DOCKER_SOCKET_PATH=/var/run/docker.sock

# GitHub OAuth (optional, for GitHub integration)
# Create an OAuth app at https://github.com/settings/developers
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret

# Google OAuth (optional)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
